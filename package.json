    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-session": "^1.17.1",
    "focus-trap": "^6.3.0",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "npm-run-all": "^4.1.5",
    "passport": "^0.4.1",
    "passport-github": "^1.1.0",
    "surge": "^0.23.0"
  },
  "devDependencies": {
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "nodemon": "^2.0.7"
  }
