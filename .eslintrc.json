{
  "extends": [
    "airbnb-base"
  ],
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2021
  },
  "rules": {
    "no-console": "warn",
    "semi": ["warn", "always"],
    "no-unused-vars": "warn",
    "max-len": ["error", 100]
  }
}
